programa {
	funcao inicio() {
	    
	    //1ª etapa variáveis
	    real area, base, altura
	    
	    //2ª etapa entrada de dados
	    escreva("Digite a base: ")
	    leia(base)
	    escreva("Digite a altura: ")
	    leia(altura)
	    
	    //3ª Etapa Processamento dos dados
	    area = (base * altura)/2
	    
	    //4ª Etapa saída de dados 
	    escreva("A base e a altura deste triangulo ficou:",area," cubico.")
		
	}
}
